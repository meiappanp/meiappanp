def count_distinct_pairs(t, K):
    pairs = set()
    seen = set()
    count = 0
    for num in t:
        complement = K - num
        if complement in seen:
            pair = (min(num, complement), max(num, complement))
            if pair not in pairs:
                pairs.add(pair)
                count += 1
        seen.add(num)
    return count

# Test cases
print(count_distinct_pairs((5, 6, 5, 7, 7, 8), 13))  
print(count_distinct_pairs((1, 2, 1, 2, 5), 3))     
print(count_distinct_pairs((1, 2), 0)) 
