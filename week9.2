def christmasDiscount(n):
    list=[]
    temp=[]
    count=0
    a=n
    while(a!=0):
        list.append(a%10)
        a=a//10
    for i in list:
        count=0
        for j in range(1,i+1):
            if i%j==0:
                count+=1
        if count==2:
            temp.append(i)
    sum=0        
    for i in temp:
        sum+=i
    return sum
